class Solution(object):
    def checkInclusion(self, s1, s2):
        """
        :type s1: str
        :type s2: str
        :rtype: bool
        """
        if len(s1) > len(s2):
            return False

        occur_s1 = [0]*26
        occur_s2 = [0]*26

        for i in range(len(s1)):  # frequency count for s1(a-0 index, b-1 index etc) and first window of s2
            occur_s1[ord(s1[i]) - ord('a')] += 1
            occur_s2[ord(s2[i]) - ord('a')] += 1

        for i in range(len(s2)-len(s1)):
            if occur_s1==occur_s2:
                return True

            occur_s2[ord(s2[i])-ord('a')] -= 1
            occur_s2[ord(s2[i + len(s1)]) - ord('a')] += 1

        return occur_s1 == occur_s2     




            

                    


