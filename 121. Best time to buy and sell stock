class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        minbuy = prices[0]
        profit = 0

        for i in range(len(prices)):
            if prices[i] < minbuy:
                minbuy = prices[i]

            profit = max(profit, prices[i]-minbuy)    

        return profit 
